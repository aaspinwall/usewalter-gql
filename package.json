{
  "scripts": {
    "dev": "run-p dev:*",
    "start": "run-s start:*",
    "dev:backend": "cd backend && yarn dev",
    "dev:frontend": "cd frontend && yarn dev",
    "start:backend": "cd backend && yarn && yarn generate",
    "start:frontend": "cd frontend && yarn",
    "test": "jest --no-cache --passWithNoTests ./*",
    "lint": "eslint '**/*.{js,jsx,ts}' .eslintignore --quiet ",
    "lint:classic": "eslint **/*.{js,jsx} --ignore-path .eslintignore --quiet --fix",
    "lint:action": "eslint . --ignore-path .eslintignore --quiet --fix"
  },
  "name": "usewalter-app",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:aaspinwall/usewalter-gql.git",
  "author": "Alejandro Aspinwall <aaspinwall@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.3.0",
    "jest": "^26.6.3",
    "prettier": "2.2.1"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "concurrently": "^5.3.0",
    "eslint": "^7.19.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "npm-run-all": "^4.1.5"
  },
